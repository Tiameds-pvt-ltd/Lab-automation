{
  "family": "tiameds-prod-task",
  "taskRoleArn": "arn:aws:iam::977098986605:role/ecsTaskExecutionRole",
  "executionRoleArn": "arn:aws:iam::977098986605:role/ecsTaskExecutionRole",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "tiameds-prod-container",
      "image": "977098986605.dkr.ecr.ap-south-1.amazonaws.com/tiameds-prod-app:latest",
      "cpu": 0,
      "memory": 3072,
      "essential": true,
      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 8080,
          "protocol": "tcp",
          "name": "tiameds-prod-container-8080-tcp",
          "appProtocol": "http"
        }
      ],

      "environment": [
        {
          "name": "SPRING_PROFILES_ACTIVE",
          "value": "prod"
        }
      ],
      "secrets": [
        {
          "name": "DB_URL",
          "valueFrom": "arn:aws:secretsmanager:ap-south-1:977098986605:secret:lab-prod-secret-dE248F:DB_URL::"
        },
        {
          "name": "DB_NAME",
          "valueFrom": "arn:aws:secretsmanager:ap-south-1:977098986605:secret:lab-prod-secret-dE248F:DB_NAME::"
        },
        {
          "name": "DB_USERNAME",
          "valueFrom": "arn:aws:secretsmanager:ap-south-1:977098986605:secret:lab-prod-secret-dE248F:DB_USERNAME::"
        },
        {
          "name": "DB_PASSWORD",
          "valueFrom": "arn:aws:secretsmanager:ap-south-1:977098986605:secret:lab-prod-secret-dE248F:DB_PASSWORD::"
        },
        {
          "name": "JWT_SECRET",
          "valueFrom": "arn:aws:secretsmanager:ap-south-1:977098986605:secret:lab-prod-secret-dE248F:JWT_SECRET::"
        },
        {
          "name": "SPRING_MAIL_USERNAME",
          "valueFrom": "arn:aws:secretsmanager:ap-south-1:977098986605:secret:lab-prod-secret-dE248F:SPRING_MAIL_USERNAME::"
        },
        {
          "name": "SPRING_MAIL_PASSWORD",
          "valueFrom": "arn:aws:secretsmanager:ap-south-1:977098986605:secret:lab-prod-secret-dE248F:SPRING_MAIL_PASSWORD::"
        },
        {
          "name": "SPRING_MAIL_SMTP_AUTH",
          "valueFrom": "arn:aws:secretsmanager:ap-south-1:977098986605:secret:lab-prod-secret-dE248F:SPRING_MAIL_SMTP_AUTH::"
        },
        {
          "name": "SPRING_MAIL_SMTP_STARTTLS_ENABLE",
          "valueFrom": "arn:aws:secretsmanager:ap-south-1:977098986605:secret:lab-prod-secret-dE248F:SPRING_MAIL_SMTP_STARTTLS_ENABLE::"
        },
        {
          "name": "SPRING_MAIL_HOST",
          "valueFrom": "arn:aws:secretsmanager:ap-south-1:977098986605:secret:lab-prod-secret-dE248F:SPRING_MAIL_HOST::"
        },
        {
          "name": "SPRING_MAIL_PORT",
          "valueFrom": "arn:aws:secretsmanager:ap-south-1:977098986605:secret:lab-prod-secret-dE248F:SPRING_MAIL_PORT::"
        },
        {
          "name": "JWT_PRIVATE_KEY_LOCATION",
          "valueFrom": "arn:aws:secretsmanager:ap-south-1:977098986605:secret:lab-prod-secret-dE248F:JWT_PRIVATE_KEY_LOCATION::"
        },
        {
          "name": "JWT_PUBLIC_KEY_LOCATION",
          "valueFrom": "arn:aws:secretsmanager:ap-south-1:977098986605:secret:lab-prod-secret-dE248F:JWT_PUBLIC_KEY_LOCATION::"
        },
        {
          "name": "JWT_ISSUER",
          "valueFrom": "arn:aws:secretsmanager:ap-south-1:977098986605:secret:lab-prod-secret-dE248F:JWT_ISSUER::"
        },
        {
          "name": "JWT_AUDIENCE",
          "valueFrom": "arn:aws:secretsmanager:ap-south-1:977098986605:secret:lab-prod-secret-dE248F:JWT_AUDIENCE::"
        },
        {
          "name": "JWT_ACCESS_TOKEN_TTL",
          "valueFrom": "arn:aws:secretsmanager:ap-south-1:977098986605:secret:lab-prod-secret-dE248F:JWT_ACCESS_TOKEN_TTL::"
        },
        {
          "name": "JWT_REFRESH_TOKEN_TTL",
          "valueFrom": "arn:aws:secretsmanager:ap-south-1:977098986605:secret:lab-prod-secret-dE248F:JWT_REFRESH_TOKEN_TTL::"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/tiameds-prod-task",
          "awslogs-region": "ap-south-1",
          "awslogs-stream-prefix": "ecs",
          "awslogs-create-group": "true"
        }
      }
    }
  ],
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "1024",
  "memory": "3072",
  "runtimePlatform": {
    "cpuArchitecture": "X86_64",
    "operatingSystemFamily": "LINUX"
  }
}
